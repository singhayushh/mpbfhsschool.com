{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "outDir": "./build",
        "typeRoots": ["./src/types", "./node_modules/@types"],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
}